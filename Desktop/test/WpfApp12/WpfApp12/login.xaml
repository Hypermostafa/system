<Page x:Class="WpfApp12.login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp12"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="login">

    <Grid Background="White">
        <TextBox x:Name="emailtextbox" HorizontalAlignment="Center" Height="47" Margin="0,178,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="370"/>
        <TextBox x:Name="passwordtextbox" HorizontalAlignment="Center" Height="47" Margin="0,247,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="370"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="44" Margin="330,374,0,0" VerticalAlignment="Top" Width="133" Click="Button_Click" />


    </Grid>
</Page>
